<%= turbo_stream_from(user, :notifications) %>
<div id="notifications" class="flex flex-col gap">
  <h1 class="text-3xl">Notifications</h1>
  <div id="<%= dom_id(user) %>-notifications-feed" class="flex flex-col gap">
    <%= render partial: "notifications/like", collection: Like.none, as: :like %>
  </div>
</div>
